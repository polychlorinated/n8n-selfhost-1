{
  "name": "n8n-monorepo",
  "version": "1.78.0",
  "private": true,
  "engines": {
    "node": ">=20.15",
    "pnpm": ">=9.15"
  },
  "packageManager": "pnpm@9.15.5",
  "scripts": {
    "preinstall": "node scripts/block-npm-install.js",
    "build": "run build",
    "build:backend": "run build:backend",
    "build:frontend": "run build:frontend",
    "build:nodes": "run build:nodes",
    "typecheck": "typecheck",
    "dev": "run dev --parallel --env-mode=loose --filter=!n8n-design-system --filter=!@n8n/chat --filter=!@n8n/task-runner",
    "dev:be": "run dev --parallel --env-mode=loose --filter=!n8n-design-system --filter=!@n8n/chat --filter=!@n8n/task-runner --filter=!n8n-editor-ui",
    "dev:ai": "run dev --parallel --env-mode=loose --filter=@n8n/nodes-langchain --filter=n8n --filter=n8n-core",
    "dev:fe": "run-p start \"dev:fe:editor --filter=n8n-design-system\"",
    "dev:fe:editor": "run dev --parallel --env-mode=loose --filter=n8n-editor-ui",
    "clean": "run clean --parallel",
    "reset": "node scripts/ensure-zx.mjs && zx scripts/reset.mjs",
    "format": "run format && node scripts/format.mjs",
    "format:check": "run format:check",
    "lint": "run lint",
    "lintfix": "run lintfix",
    "optimize-svg": "find ./packages -name '*.svg' ! -name 'pipedrive.svg' -print0 | xargs -0 -P16 -L20 npx svgo",
    "start": "node packages/cli/bin/n8n start",
    "start:tunnel": "node packages/cli/bin/n8n start --tunnel",
    "test": "run test",
    "test:backend": "run test:backend --concurrency=1",
    "test:frontend": "run test:frontend --concurrency=1",
    "test:nodes": "run test:nodes --concurrency=1",
    "watch": "run watch --parallel",
    "webhook": "node packages/cli/bin/n8n webhook",
    "worker": "node packages/cli/bin/n8n worker"
  },
  "devDependencies": {
    "@types/node": "*",
    "typescript": "*"
  },
  "dependencies": {
    "semver": "^7.5.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sqlite3"
    ],
    "overrides": {
      "@types/node": "^18.16.16",
      "chokidar": "^4.0.1",
      "esbuild": "^0.24.0",
      "formidable": "3.5.1",
      "pug": "^3.0.3",
      "semver": "^7.5.4",
      "tslib": "^2.6.2",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.7.2",
      "vue-tsc": "^2.1.10",
      "ws": ">=8.17.1"
    },
    "patchedDependencies": {
      "bull@4.12.1": "patches/bull@4.12.1.patch",
      "pkce-challenge@3.0.0": "patches/pkce-challenge@3.0.0.patch",
      "pyodide@0.23.4": "patches/pyodide@0.23.4.patch",
      "@types/express-serve-static-core@4.17.43": "patches/@types__express-serve-static-core@4.17.43.patch",
      "@types/ws@8.5.4": "patches/@types__ws@8.5.4.patch",
      "@types/uuencode@0.0.3": "patches/@types__uuencode@0.0.3.patch",
      "vue-tsc@2.1.10": "patches/vue-tsc@2.1.10.patch"
    }
  }
}
